{
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "noEmitOnError": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "paths": {
      "@services/*": ["services/*"]
    }
  }
}
